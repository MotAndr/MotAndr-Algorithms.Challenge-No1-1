pi=3.14
x=0
k=0
b=10
x1 = input("Введіть х1 ")
x1 = int(x1) # перетворення в ціле число
y1 = input("Введіть у1 ")
y1 = int(y1) # перетворення в ціле число
x2 = input("Введіть х2 ")
x2 = int(x2) # перетворення в ціле число
y2 = input("Введіть у2 ")
y2 = int(y2) # перетворення в ціле число
OX=k*x+b    
OA=x1*x+y1   # рівняння відрізка ОА
OB=x2*x+y2   # рівняння відрізка ОВ
Q=(x1-x)/(1+x+x1)
W=(x2-x)/(1+x+x2)
R=(Q*180)/pi # переводимо із радіан в градуси
T=(W*180)/pi # переводимо із радіан в градуси
print (R)
print (T)
if R<T : print (T)
if T<R : print (R)
